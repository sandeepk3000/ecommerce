                    <!-- Slides -->
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img src="/img/close-up-colorful-books-pile.jpg" class="d-block w-100" style="height: 40rem;" alt="Image 1">
                            <div class="carousel-caption">
                                <h3>Slide 1</h3>
